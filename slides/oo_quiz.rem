# Common mistake #2

$10 - 4 - 1$


--

.left-column[

$= 10 - 3$


$= 7$
]


--

.right-column[


$= 6 - 1$


$= 5$
]

---
# Common mistake #3
$2^2+(-20) \div 4$

--

.left-column[
$4+(-20) \div 4$


$=-16 \div 4$


$= -4$
]


--

.right-column[
$4+(-20)\div 4$


$= 4 + (-5)$


$= -1$
]



---
# Common mistake #4
$(-40) \div (-4) \cdot 2$

--

.left-column[
$= 10 \cdot 2$


$=20$

]


--

.right-column[
$= (-40) \div (-8)$


$=5$


$= -1$
]




